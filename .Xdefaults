urxvt*font: xft:Noto Sans Mono :size=9, xft:Terminus:pixelsize=1:antialias=false
#xft:Noto Sans Mono :size=8, xft:Kochi Gothic :antialias=false, xft:FontAwesome :size=8
urxvt*scrollBar: false
urxvt*transparent: true
urxvt*tintColor: white
urxvt*shading: 30
urxvt*background: #1b1918
urxvt*foreground: #ffffff
urxvt*fading: 30
urxvt*fadeColor: #000000

urxvt*iso14755: false
urxvt*iso14755_52: false

urxvt*perl-ext-common: default,clipboard,matcher,resize-font
urxvt*keysym.C-C: perl:clipboard:copy
urxvt*keysym.C-V: perl:clipboard:paste
urxvt*url-launcher: /usr/bin/firefox
urxvt*matcher.button: 1
urxvt*resize-font.smaller: C-Down
urxvt*resize-font.bigger: C-Up
#URxvt.keysym.Control-l: printf "\\033c"\n

urxvt.keysym.Control-l: command:\033c
